{
  "name": "windows-os-web",
  "version": "1.0.2",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "commentsForBuildScript": "https://github.com/aws-amplify/amplify-hosting/issues/3872#issuecomment-1980806305",
    "dev": "env-cmd -f env/.env-dev next dev -p 3002",
    "start": "next start",
    "clean": "rm -rf public/.index/",
    "build": "env-cmd --verbose -f env/.env-production next build",
    "build:fs": "node scripts/fs2json.js --exclude .index --out public/.index/fs.9p.json ./public",
    "build:prebuild": "node scripts/searchIndex.js && node scripts/preloadIcons.js && node scripts/cacheShortcuts.js && npm run build:fs",
    "build:analyze": "env-cmd --verbose -f env/.env-analyze next build",
    "prettier": "prettier . --write",
    "lint": "next lint",
    "find-deadcode": "ts-prune --error"
  },
  "dependencies": {
    "@uiw/color-convert": "^2.3.2",
    "@uiw/react-color-sketch": "^2.3.2",
    "browserfs": "^1.4.3",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "downshift": "^9.0.8",
    "es-toolkit": "^1.21.0",
    "fix-webm-duration": "^1.0.6",
    "framer-motion": "^11.3.8",
    "gif.js.optimized": "^1.0.1",
    "gl-matrix": "^3.4.3",
    "html-to-image": "^1.11.11",
    "idb": "^8.0.0",
    "ini": "^4.1.3",
    "lunr": "^2.3.9",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-rnd": "^10.4.12",
    "react-zoom-pan-pinch": "^3.6.1",
    "sass": "^1.77.8",
    "sharp": "^0.33.5",
    "simplebar-react": "^3.2.6",
    "simplex-noise": "^4.0.3",
    "worker-timers": "^8.0.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.15",
    "@types/gif.js": "^0.2.5",
    "@types/ini": "^4.1.1",
    "@types/lunr": "^2.3.7",
    "@types/node": "^20.16.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/video.js": "^7.3.58",
    "@types/wicg-file-system-access": "^2023.10.5",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8",
    "prettier": "^3.3.1",
    "prettier-plugin-classnames": "^0.7.3",
    "prettier-plugin-merge": "^0.7.1",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.1",
    "ts-prune": "^0.10.3",
    "typescript": "^5"
  }
}
